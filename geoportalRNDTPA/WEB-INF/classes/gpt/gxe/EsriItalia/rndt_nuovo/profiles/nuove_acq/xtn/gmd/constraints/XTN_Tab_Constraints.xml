<?xml version="1.0" encoding="UTF-8"?>
<!--  INSPIRE Identification Constraints tab (gmd:resourceConstraints) --> 
<h:div xmlns:g="http://www.esri.com/geoportal/gxe" 
       xmlns:h="http://www.esri.com/geoportal/gxe/html" 
       h:tag="div" 
       g:label="$i18n.catalog.mdParam.rndt.constraints.caption">

	<!-- gmd:resourceConstraints/gmd:MD_Constraints -->
	<g:element g:targetName="gmd:resourceConstraints" g:minOccurs="1" g:maxOccurs="1"
    h:tag="div" g:jsClass="gxe.control.Element">
		<g:matchTopElements> 
			<g:match g:qPath="gmd:MD_Constraints" g:qMode="must"/>
		</g:matchTopElements>
		<g:element g:targetName="gmd:MD_Constraints" g:label="$i18n.catalog.mdParam.rndt.constraints.limitations.caption"  g:minOccurs="1" g:maxOccurs="1" g:extends="$base/xtn/ui/UI_Element.xml">
			<g:body>
				<!-- use limitation -->
						<g:element g:targetName="gmd:useLimitation" g:info="$i18n.catalog.rdnt.nuoveAcquisizioni.hint.testoLiberoV" g:label="$i18n.catalog.mdParam.rndt.constraints.conditions.free.caption" g:minOccurs="1" g:maxOccurs="1"
					g:extends="$base/xtn/ui/UI_Element.xml">
							<g:body>
								<g:element g:targetName="gco:CharacterString" g:extends="$base/core/ui/InputTextArea.xml" h:cols="60" h:rows="8"/>
							</g:body>
						</g:element>  
			</g:body>
		</g:element>
	</g:element>
</h:div>     

