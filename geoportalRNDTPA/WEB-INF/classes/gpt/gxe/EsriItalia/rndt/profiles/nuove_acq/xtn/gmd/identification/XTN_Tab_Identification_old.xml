<?xml version="1.0" encoding="UTF-8"?>   
<g:element xmlns:g="http://www.esri.com/geoportal/gxe" 
    xmlns:h="http://www.esri.com/geoportal/gxe/html"
	g:targetName="gmd:MD_DataIdentification"
	g:extends="$base/xtn/ui/UI_Tabs.xml">
				<g:body>
				<!-- Primo Tab citation-->
					<g:element g:targetName="gmd:citation" g:label="$i18n.catalog.mdParam.citation.caption" g:minOccurs="1" g:maxOccurs="1" h:tag="div" g:jsClass="gxe.control.Element">
						<g:element g:targetName="gmd:CI_Citation" g:minOccurs="1" g:maxOccurs="1" h:tag="div" g:jsClass="gxe.control.Element"> 

							<!-- Citation tab **{1}-->
							<h:div g:extends="$base/xtn/gmd/citation/XTN_Tab_Citation.xml"/>

							<h:div g:label="$i18n.catalog.mdParam.rndt.identification.contact.caption">
								<g:import g:src="$base/xtn/gmd/citation/XTN_Cited_Responsible_Party.xml"/>
							</h:div>    


							<g:element g:minOccurs="1" g:targetName="gmd:presentationForm" g:maxOccurs="unbounded" xmlns:g="http://www.esri.com/geoportal/gxe" 
				xmlns:h="http://www.esri.com/geoportal/gxe/html"
				g:extends="$base/schema/gco/xtn/Wrapped_CodePropertyType.xml"
				g:label="$i18n.catalog.mdParam.rndt.identification.presentationForm">       
								<g:body>
									<g:import g:src="$base/schema/gmd/citation/CI_PresentationFormCode.xml"/>
								</g:body>
							</g:element>
							<g:element g:targetName="gmd:otherCitationDetails" g:minOccurs="0" g:maxOccurs="1" 
                h:tag="div" g:jsClass="gxe.control.Element" g:label="$i18n.catalog.mdParam.rndt.identification.service" 
                g:extends="$base/schema/gco/basicTypes/CharacterString_PropertyType.xml">    
							</g:element>
						</g:element>

					</g:element>
				<!-- Secondo Tab citation-->
					<h:div g:extends="$base/xtn/gmd/citation/XTN_Tab_Citation2.xml"/>
				<!-- Thumb-->

					<!--VINCOLI**{1}-->
					<h:div g:extends="$base/xtn/gmd/constraints/XTN_Tab_Constraints.xml"/>
					<!--Rappresentazione e risoluzione spaziale tab **{1}-->
					<h:div g:label="$i18n.catalog.mdParam.rndt.identification.resolution.caption">
						<g:element g:targetName="gmd:spatialRepresentationType" g:minOccurs="1" g:maxOccurs="unbounded" g:label="$i18n.catalog.mdParam.rndt.identification.resolution.representationType" g:extends="$base/schema/gmd/identification/MD_SpatialRepresentationTypeCode_PropertyType.xml"/>

						<!--tolto da NICOL<g:element g:extends="$base/xtn/gmd/referenceSystem/XTN_RsIdentifierCode.xml"/>   **{1}-->	
						<g:element g:targetName="gmd:spatialResolution" g:minOccurs="1" g:maxOccurs="unbounded" 
					   g:label="$i18n.catalog.iso19139.XTN_Identification.spatialResolution" 
					   g:extends="$base/schema/gmd/identification/MD_Resolution_PropertyType.xml"/>
					</h:div>
					<h:div g:label="$i18n.catalog.mdParam.rndt.identification.topics"> 
						<g:element g:extends="$base/xtn/gmd/metadataEntity/XTN_Language.xml" g:minOccurs="1" h:class="divConBordi" g:maxOccurs="unbounded" 
							g:label="$i18n.catalog.mdParam.rndt.identification.language"/>
						<g:element g:targetName="gmd:topicCategory" g:minOccurs="1" g:maxOccurs="unbounded"
                    g:extends="$base/schema/gmd/identification/MD_TopicCategoryCode_PropertyType.xml" g:label="$i18n.catalog.mdParam.rndt.identification.topics"/>


					</h:div>



					<!--{1}** Extension data tab **{1}-->
					<h:div g:label="$i18n.catalog.mdParam.rndt.extent.caption">
						<g:element g:targetName="gmd:extent" g:minOccurs="0" g:maxOccurs="1"
						 h:tag="div" g:jsClass="gxe.control.Element">
							<g:element g:targetName="gmd:EX_Extent" g:minOccurs="0" g:maxOccurs="1"
						   h:tag="div" g:jsClass="gxe.control.Element">
								<g:element g:extends="$base/xtn/gmd/extent/XTN_GeographicElement.xml"/>
                                                                <!-- =====================-->
                        					<!-- LIMITE AMMINISTRATIVO EX_GeographicDescription-->
                                                                <g:element g:targetName="gmd:geographicElement" minOccurs="0"  maxOccurs="unbounded"
                                                                  g:extends="$base/schema/gmd/extent/EX_GeographicDescription.xml">
                                                                </g:element>
							</g:element>
						</g:element>        
					</h:div>      
					<h:div g:label="$i18n.catalog.mdParam.identification.program">
						<g:element 
							   g:targetName="gmd:supplementalInformation" g:minOccurs="0" g:maxOccurs="1"
							   g:label="$i18n.catalog.mdParam.identification.program"
							   g:extends="$base/schema/gco/basicTypes/CharacterString_PropertyType.xml">
							<g:body>
								<g:element>
									<g:body>
										<g:input g:extends="$base/core/ui/InputTextArea.xml" h:cols="60" h:rows="8"/>
									</g:body>
								</g:element>
							</g:body>
						</g:element>  
					</h:div>
				</g:body>
</g:element>

