<?xml version="1.0" encoding="UTF-8"?>
<!-- INSPIRE data theme keywords (gmd:descriptiveKeywords) --> 
<g:element xmlns:g="http://www.esri.com/geoportal/gxe" 
           xmlns:h="http://www.esri.com/geoportal/gxe/html" 
           h:class=""
           g:targetName="gmd:descriptiveKeywords" g:minOccurs="1" g:maxOccurs="1"
           g:extends="$base/xtn/ui/UI_Element.xml">
  <g:header g:rendered="false"/>
  <g:body>
	  <g:element g:targetName="gmd:MD_Keywords" g:minOccurs="0" g:maxOccurs="1"
	    h:tag="div" g:jsClass="gxe.control.Element">
	    
	    <!-- keyword selection -->
	    <g:element g:targetName="gmd:keyword" g:minOccurs="1" g:maxOccurs="unbounded"
	      g:label="$i18n.catalog.mdParam.inspire.keyword.dataTheme.label"
		  g:info="$i18n.catalog.rdnt.dataset.hint.temaInspireTI"
	      g:extends="$base/schema/gco/basicTypes/CharacterString_PropertyType.xml">
	      <g:body>
	        <g:element g:isIsoWMVL="true">
	          <g:body>
	            <g:input g:extends="$base/core/ui/InputSelectMany.xml">
	              <g:options g:i18nBase="catalog.mdParam.inspire.keyword.dataTheme">
		            <g:option g:value="$i18nBase.atmosphericConditions" g:alias="Atmospheric conditions" g:label="$i18nBase.atmosphericConditions"/>
			        <g:option g:value="$i18nBase.landCover" g:alias="Land cover" g:label="$i18nBase.landCover"/>
			        <g:option g:value="$i18nBase.populationDistributionDemography" g:alias="Population distribution â€” demography" g:label="$i18nBase.populationDistributionDemography"/>
			        <g:option g:value="$i18nBase.speciesDistribution" g:alias="Species distribution" g:label="$i18nBase.speciesDistribution"/>
		            <g:option g:value="$i18nBase.buildings" g:alias="Buildings" g:label="$i18nBase.buildings"/>
			        <g:option g:value="$i18nBase.meteorologicalGeographicalFeatures" g:alias="Meteorological geographical features" g:label="$i18nBase.meteorologicalGeographicalFeatures"/>
			        <g:option g:value="$i18nBase.oceanographicGeographicalFeatures" g:alias="Oceanographic geographical features" g:label="$i18nBase.oceanographicGeographicalFeatures"/>
			        <g:option g:value="$i18nBase.elevation" g:alias="Elevation" g:label="$i18nBase.elevation"/>
			        <g:option g:value="$i18nBase.geology" g:alias="Geology" g:label="$i18nBase.geology"/>
			        <g:option g:value="$i18nBase.habitatsAndBiotopes" g:alias="Habitats and biotopes" g:label="$i18nBase.habitatsAndBiotopes"/>
			        <g:option g:value="$i18nBase.hydrography" g:alias="Hydrography" g:label="$i18nBase.hydrography"/>
		            <g:option g:value="$i18nBase.agriculturalAndAquacultureFacilities" g:alias="Agricultural and aquaculture facilities" g:label="$i18nBase.agriculturalAndAquacultureFacilities"/>
			        <g:option g:value="$i18nBase.environmentalMonitoringFacilities" g:alias="Environmental monitoring facilities" g:label="$i18nBase.environmentalMonitoringFacilities"/>
					<g:option g:value="$i18nBase.addresses" g:alias="Addresses" g:label="$i18nBase.addresses"/>
			        <g:option g:value="$i18nBase.geographicalNames" g:alias="Geographical names" g:label="$i18nBase.geographicalNames"/>
			        <g:option g:value="$i18nBase.orthoimagery" g:alias="Orthoimagery" g:label="$i18nBase.orthoimagery"/>
		            <g:option g:value="$i18nBase.cadastralParcels" g:alias="Cadastral parcels" g:label="$i18nBase.cadastralParcels"/>
			        <g:option g:value="$i18nBase.productionAndIndustrialFacilities" g:alias="Production and industrial facilities" g:label="$i18nBase.productionAndIndustrialFacilities"/>
		            <g:option g:value="$i18nBase.bioGeographicalRegions" g:alias="Bio-geographical regions" g:label="$i18nBase.bioGeographicalRegions"/>
			        <g:option g:value="$i18nBase.seaRegions" g:alias="Sea regions" g:label="$i18nBase.seaRegions"/>
			        <g:option g:value="$i18nBase.transportNetworks" g:alias="Transport networks" g:label="$i18nBase.transportNetworks"/>
			        <g:option g:value="$i18nBase.energyResources" g:alias="Energy resources" g:label="$i18nBase.energyResources"/>
			        <g:option g:value="$i18nBase.mineralResources" g:alias="Mineral resources" g:label="$i18nBase.mineralResources"/>
			        <g:option g:value="$i18nBase.humanHealthAndSafety" g:alias="Human health and safety" g:label="$i18nBase.humanHealthAndSafety"/>
			        <g:option g:value="$i18nBase.utilityAndGovernmentService" g:alias="Utility and Government services" g:label="$i18nBase.utilityAndGovernmentService"/>       
	                <g:option g:value="$i18nBase.coordinateReferenceSystems" g:alias="Coordinate reference systems" g:label="$i18nBase.coordinateReferenceSystems"/>
			        <g:option g:value="$i18nBase.geographicalGridSystems" g:alias="Geographical grid systems" g:label="$i18nBase.geographicalGridSystems"/>
			        <g:option g:value="$i18nBase.protectedSites" g:alias="Siti protetti" g:label="$i18nBase.protectedSites"/>
			        <g:option g:value="$i18nBase.soil" g:alias="Suolo" g:label="$i18nBase.soil"/>
					<g:option g:value="$i18nBase.administrativeUnits" g:alias="Administrative units" g:label="$i18nBase.administrativeUnits"/>
			        <g:option g:value="$i18nBase.statisticalUnits" g:alias="Statistical units" g:label="$i18nBase.statisticalUnits"/>
			        <g:option g:value="$i18nBase.landUser" g:alias="Land use" g:label="$i18nBase.landUser"/>
			        <g:option g:value="$i18nBase.naturalRiskZones" g:alias="Natural risk zones" g:label="$i18nBase.naturalRiskZones"/>
		            <g:option g:value="$i18nBase.areaManagementRestrictionRegulationZonesAndReportingUnits" g:alias="Area management/restriction/regulation zones and reporting units" g:label="$i18nBase.areaManagementRestrictionRegulationZonesAndReportingUnits"/>
		            </g:options>
		          </g:input>
		        </g:body>
		      </g:element>
		    </g:body> 
	    </g:element>

	    
	    <!-- GEMET - INSPIRE themes, version 1.0 -->
	    <g:element g:targetName="gmd:thesaurusName" 
	      h:tag="div" h:class="gxeDisplayNone" g:jsClass="gxe.control.Element">
	      <g:element g:targetName="gmd:CI_Citation" h:tag="div" g:jsClass="gxe.control.Element">
	        <g:element g:targetName="gmd:title" h:tag="div" g:jsClass="gxe.control.Element">
	          <g:element g:targetName="gco:CharacterString" h:tag="div" g:jsClass="gxe.control.Element"
	            g:fixedValue="GEMET - INSPIRE themes, version 1.0">
	            <g:input g:extends="$base/core/ui/InputText.xml"/>
	          </g:element>
	        </g:element>
	        <g:element g:targetName="gmd:date" h:tag="div" g:jsClass="gxe.control.Element">
	          <g:element g:targetName="gmd:CI_Date" h:tag="div" g:jsClass="gxe.control.Element">
	            <g:element g:targetName="gmd:date" h:tag="div" g:jsClass="gxe.control.Element"> 
	             <g:element g:targetName="gco:Date" h:tag="div" g:jsClass="gxe.control.Element"
	               g:fixedValue="2008-06-01">
	               <g:input g:extends="$base/core/ui/InputText.xml"/>
	             </g:element>
	           </g:element>
	            <g:element g:targetName="gmd:dateType" h:tag="div" g:jsClass="gxe.control.Element">
	             <g:element g:targetName="gmd:CI_DateTypeCode" h:tag="div" g:jsClass="gxe.control.Element"
	               g:fixedValue="publication">
	               <g:input g:extends="$base/core/ui/InputText.xml"/>
	               <g:attribute g:targetNS="" g:targetName="codeList" h:tag="div" g:jsClass="gxe.control.Attribute"
	                 g:fixedValue="http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19139_Schemas/resources/codelist/gmxCodelists.xml#CI_DateTypeCode">
	                 <g:input g:extends="$base/core/ui/InputText.xml"/>
	               </g:attribute>
	               <g:attribute g:targetNS="" g:targetName="codeListValue" h:tag="div" g:jsClass="gxe.control.Attribute"
	                 g:fixedValue="publication">
	                 <g:input g:extends="$base/core/ui/InputText.xml"/>
	               </g:attribute>
	             </g:element>
	           </g:element>
	          </g:element>
	        </g:element>
	      </g:element> 
	    </g:element>
	  
	  </g:element>
	</g:body>
</g:element>
  
      

