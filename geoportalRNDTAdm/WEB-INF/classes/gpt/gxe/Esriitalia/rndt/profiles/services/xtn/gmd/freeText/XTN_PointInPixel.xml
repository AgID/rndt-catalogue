<?xml version="1.0" encoding="UTF-8"?> 
<g:element xmlns:g="http://www.esri.com/geoportal/gxe" 
           xmlns:h="http://www.esri.com/geoportal/gxe/html"
           g:targetName="gmd:pointInPixel" 
           g:extends="$base/schema/gco/xtn/Wrapped_CodePropertyType.xml">
  <g:body>
 <g:element g:targetName="gmd:MD_PixelOrientationCode" g:minOccurs="1" g:maxOccurs="1"
      h:tag="div" g:jsClass="gxe.control.Element">
        <g:input g:extends="$base/core/ui/InputSelectOne.xml">
          <g:options g:i18nBase="catalog.mdCode.rndt.MD_PixelOrientationCode">
            <g:option g:value="center" g:label="$i18nBase.center"/>
            <g:option g:value="lowerLeft" g:label="$i18nBase.lowerLeft"/>
            <g:option g:value="lowerRight" g:label="$i18nBase.lowerRight"/>
            <g:option g:value="upperRight" g:label="$i18nBase.upperRight"/>
            <g:option g:value="upperLeft" g:label="$i18nBase.upperLeft"/>
           </g:options>
        </g:input>
   </g:element>
  </g:body> 
</g:element>
