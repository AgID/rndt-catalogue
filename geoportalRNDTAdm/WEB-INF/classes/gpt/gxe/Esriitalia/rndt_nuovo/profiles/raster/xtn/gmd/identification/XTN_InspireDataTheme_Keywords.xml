<?xml version="1.0" encoding="UTF-8"?>
<!-- INSPIRE data theme keywords (gmd:descriptiveKeywords) --> 
<g:element xmlns:g="http://www.esri.com/geoportal/gxe" 
           xmlns:h="http://www.esri.com/geoportal/gxe/html" 
		   xmlns:xlink="http://www.w3.org/1999/xlink" 
		   xmlns:gmx="http://www.isotc211.org/2005/gmx"
           h:class=""
           g:targetName="gmd:descriptiveKeywords" g:minOccurs="1" g:maxOccurs="1"
           g:extends="$base/xtn/ui/UI_Element.xml">
	<g:header g:rendered="false"/>
	<g:body>
		<g:element g:targetName="gmd:MD_Keywords" g:minOccurs="0" g:maxOccurs="1"
	    h:tag="div" g:jsClass="gxe.control.Element">

			<!-- keyword selection -->
			<g:element g:targetName="gmd:keyword" g:minOccurs="1" g:maxOccurs="unbounded"
	      g:label="$i18n.catalog.mdParam.inspire.keyword.dataTheme.label"
		  g:info="$i18n.catalog.rdnt.dataset.hint.temaInspireTI"
		  g:extends="$base/schema/gco/basicTypes/CharacterString_PropertyType.xml">
				<g:body>
					<g:element g:targetName="gmx:Anchor" g:isIsoWMVL="true">
						<g:body>
							<g:input g:extends="$base/core/ui/InputSelectMany.xml">
								<g:options g:i18nBase="catalog.mdParam.inspire.keyword.dataTheme">
									<g:option g:value="http://inspire.ec.europa.eu/theme/ac" g:alias="Atmospheric conditions" g:label="$i18nBase.atmosphericConditions"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/lc" g:alias="Land cover" g:label="$i18nBase.landCover"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/pd" g:alias="Population distribution â€” demography" g:label="$i18nBase.populationDistributionDemography"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/sd" g:alias="Species distribution" g:label="$i18nBase.speciesDistribution"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/bu" g:alias="Buildings" g:label="$i18nBase.buildings"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/mf" g:alias="Meteorological geographical features" g:label="$i18nBase.meteorologicalGeographicalFeatures"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/of" g:alias="Oceanographic geographical features" g:label="$i18nBase.oceanographicGeographicalFeatures"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/el" g:alias="Elevation" g:label="$i18nBase.elevation"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/ge" g:alias="Geology" g:label="$i18nBase.geology"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/hb" g:alias="Habitats and biotopes" g:label="$i18nBase.habitatsAndBiotopes"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/hy" g:alias="Hydrography" g:label="$i18nBase.hydrography"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/af" g:alias="Agricultural and aquaculture facilities" g:label="$i18nBase.agriculturalAndAquacultureFacilities"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/ef" g:alias="Environmental monitoring facilities" g:label="$i18nBase.environmentalMonitoringFacilities"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/ad" g:alias="Addresses" g:label="$i18nBase.addresses"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/gn" g:alias="Geographical names" g:label="$i18nBase.geographicalNames"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/oi" g:alias="Orthoimagery" g:label="$i18nBase.orthoimagery"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/cp" g:alias="Cadastral parcels" g:label="$i18nBase.cadastralParcels"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/pf" g:alias="Production and industrial facilities" g:label="$i18nBase.productionAndIndustrialFacilities"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/br" g:alias="Bio-geographical regions" g:label="$i18nBase.bioGeographicalRegions"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/sr" g:alias="Sea regions" g:label="$i18nBase.seaRegions"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/tn" g:alias="Transport networks" g:label="$i18nBase.transportNetworks"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/er" g:alias="Energy resources" g:label="$i18nBase.energyResources"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/mr" g:alias="Mineral resources" g:label="$i18nBase.mineralResources"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/hh" g:alias="Human health and safety" g:label="$i18nBase.humanHealthAndSafety"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/us" g:alias="Utility and Government services" g:label="$i18nBase.utilityAndGovernmentService"/>       
									<g:option g:value="http://inspire.ec.europa.eu/theme/rs" g:alias="Coordinate reference systems" g:label="$i18nBase.coordinateReferenceSystems"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/gg" g:alias="Geographical grid systems" g:label="$i18nBase.geographicalGridSystems"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/ps" g:alias="Siti protetti" g:label="$i18nBase.protectedSites"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/so" g:alias="Suolo" g:label="$i18nBase.soil"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/au" g:alias="Administrative units" g:label="$i18nBase.administrativeUnits"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/su" g:alias="Statistical units" g:label="$i18nBase.statisticalUnits"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/lu" g:alias="Land use" g:label="$i18nBase.landUser"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/nz" g:alias="Natural risk zones" g:label="$i18nBase.naturalRiskZones"/>
									<g:option g:value="http://inspire.ec.europa.eu/theme/am" g:alias="Area management/restriction/regulation zones and reporting units" g:label="$i18nBase.areaManagementRestrictionRegulationZonesAndReportingUnits"/>
								</g:options>
							</g:input>
						</g:body>
					</g:element>
				</g:body> 
			</g:element>


			<g:element g:targetName="gmd:thesaurusName" 
	      h:tag="div" h:class="gxeDisplayNone" g:jsClass="gxe.control.Element">
				<g:element g:targetName="gmd:CI_Citation" h:tag="div" g:jsClass="gxe.control.Element">
					<g:element g:targetName="gmd:title" h:tag="div" g:jsClass="gxe.control.Element">
						<g:element g:targetName="gmx:Anchor" h:tag="div" g:jsClass="gxe.control.Element" g:extends="$base/xtn/ui/UI_Element.xml" g:fixedValue="GEMET - INSPIRE themes, version 1.0">
							<!--GEMET - INSPIRE themes, version 1.0-->
							<g:input g:extends="$base/core/ui/InputText.xml"/>
							<g:attribute g:targetNS="" g:targetName="xlink:href" h:tag="div" g:jsClass="gxe.control.Attribute"
					g:fixedValue="http://www.eionet.europa.eu/gemet/inspire_themes">
								<g:input g:extends="$base/core/ui/InputText.xml"/>
							</g:attribute>
						</g:element>
					</g:element>
					<g:element g:targetName="gmd:date" h:tag="div" g:jsClass="gxe.control.Element">
						<g:element g:targetName="gmd:CI_Date" h:tag="div" g:jsClass="gxe.control.Element">
							<g:element g:targetName="gmd:date" h:tag="div" g:jsClass="gxe.control.Element"> 
								<g:element g:targetName="gco:Date" h:tag="div" g:jsClass="gxe.control.Element"
	               g:fixedValue="2008-06-01">
									<g:input g:extends="$base/core/ui/InputText.xml"/>
								</g:element>
							</g:element>
							<g:element g:targetName="gmd:dateType" h:tag="div" g:jsClass="gxe.control.Element">
								<g:element g:targetName="gmd:CI_DateTypeCode" h:tag="div" g:jsClass="gxe.control.Element"
	               g:fixedValue="publication">
									<g:input g:extends="$base/core/ui/InputText.xml"/>
									<g:attribute g:targetNS="" g:targetName="codeList" h:tag="div" g:jsClass="gxe.control.Attribute"
	                 g:fixedValue="http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19139_Schemas/resources/codelist/gmxCodelists.xml#CI_DateTypeCode">
										<g:input g:extends="$base/core/ui/InputText.xml"/>
									</g:attribute>
									<g:attribute g:targetNS="" g:targetName="codeListValue" h:tag="div" g:jsClass="gxe.control.Attribute"
	                 g:fixedValue="publication">
										<g:input g:extends="$base/core/ui/InputText.xml"/>
									</g:attribute>
								</g:element>
							</g:element>
						</g:element>
					</g:element>
				</g:element> 
			</g:element>

		</g:element>
	</g:body>
</g:element>



