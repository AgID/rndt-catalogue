<?xml version="1.0" encoding="UTF-8"?> 
<!-- Metadata tab (Dataset) -->        
<h:div xmlns:g="http://www.esri.com/geoportal/gxe" 
       xmlns:h="http://www.esri.com/geoportal/gxe/html" 
       h:tag="div">
	<g:tabs g:extends="$base/xtn/ui/UI_Tabs.xml">
		<g:body>
			<h:div g:label="$i18n.catalog.mdParam.rndt.identification.contact.caption">
				<g:element g:targetName="gmd:identificationInfo" g:minOccurs="0" g:maxOccurs="1" h:tag="div" g:jsClass="gxe.control.Element">
					<g:element g:targetName="gmd:MD_DataIdentification" g:minOccurs="0" g:maxOccurs="1" h:tag="div" g:jsClass="gxe.control.Element"> 
						<g:element g:targetName="gmd:citation" g:minOccurs="0" g:maxOccurs="1" h:tag="div" g:jsClass="gxe.control.Element">
							<g:element g:targetName="gmd:CI_Citation" g:minOccurs="0" g:maxOccurs="1" h:tag="div" g:jsClass="gxe.control.Element"> 

								<g:import g:src="$base/xtn/gmd/citation/XTN_Cited_Responsible_Party.xml"/>
							</g:element>
						</g:element>
					</g:element>
				</g:element>
			</h:div>    

		</g:body>
	</g:tabs>
</h:div>
